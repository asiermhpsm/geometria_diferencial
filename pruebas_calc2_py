import tkinter as tk
from ttkwidgets.autocomplete import AutocompleteCombobox

# Crear la ventana principal
root = tk.Tk()
root.title("Ejemplo de AutocompleteCombobox")

# Crear un AutocompleteCombobox
elementos = ["Manzana", "Banana", "Cereza", "Damasco", "Uva", "Kiwi"]
combo = AutocompleteCombobox(root, completevalues=elementos)
combo.pack()


# Iniciar la aplicaci√≥n
root.mainloop()
